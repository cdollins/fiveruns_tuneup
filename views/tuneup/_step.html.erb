<li>
  <dl>
    <dt><p><span class='time'><%= '%.2f' % step.time %> ms</span><%= tuneup_step_link step %></p></dt>
    <dd><%= tuneup_step_bar(step) %></dd>
  </dl>
  <% if step.children.any? %>  
    <ul>
      <%= render :partial => 'step', :collection => step.children %>
    </ul>
  <% end %>
</li>